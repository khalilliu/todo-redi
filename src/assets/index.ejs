<!DOCTYPE html>
<html lang="en" data-framework="typescript">
  <head>
    <meta charset="utf-8" />
    <title>redi â€¢ starter</title>
  </head>

  <body>
    <div id="todoapp" class="todoapp"></div>
    <footer class="info">
      <p>Double-click to edit a todo</p>
      <p>
        Created by&nbsp;<a href="http://github.com/wendellhu95/">Wendell Hu</a>
      </p>
      <p>Part of&nbsp;<a href="http://todomvc.com">TodoMVC</a></p>
    </footer>
    <script>
      var isPc = (function () {
        var userAgentInfo = navigator.userAgent
        if (/(iPhone|iPad|iPod|iOS)/i.test(userAgentInfo)) {
          return false
        } else if (/(Android)/i.test(userAgentInfo)) {
          return false
        } else {
          return true
        }
      })()
      var script = document.createElement('script')
      if (isPc) {
        script.src = '<%= htmlWebpackPlugin.files.js[0]%>'
      } else {
        script.src = '<%= htmlWebpackPlugin.files.js[1]%>'
      }
      document.body.appendChild(script)
    </script>
  </body>
</html>
